{
  "name" : "los/api-server",
  "description" : "PHP api server middleware",
  "require" : {
    "php" : "^7.0",
    "psr/http-message" : "~1.0",
    "container-interop/container-interop" : "~1.1",
    "los/api-problem" : "dev-develop",
    "nocarrier/hal" : "^0.9.12",
    "zendframework/zend-inputfilter" : "^2.7",
    "zendframework/zend-paginator" : "^2.7",
    "zendframework/zend-eventmanager" : "^3.0"
  },
  "require-dev" : {
    "phpunit/phpunit" : "^5.0 | ^6.0"
  },
  "minimum-stability": "alpha",
  "license" : "MIT",
  "autoload-dev" : {
    "psr-4" : {
      "LosMiddleware\\ApiServerTest\\" : "test/"
    }
  },
  "autoload" : {
    "psr-4" : {
      "LosMiddleware\\ApiServer\\" : "src/"
    }
  },
  "type" : "library",
  "scripts" : {
    "test" : [ "php vendor/squizlabs/php_codesniffer/scripts/phpcs", "php vendor/phpunit/phpunit/phpunit" ]
  },
  "homepage" : "https://github.com/lansoweb/api-server",
  "authors" : [ {
    "name" : "Leandro Silva",
    "homepage" : "http://leandrosilva.info/"
  } ]
}